                            android:layout_height="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/margin_padding_10dp"
                            android:layout_marginTop="@dimen/margin_padding_10dp"
                            android:src="@mipmap/btn_xx"
                            android:visibility="gone" />

                        <ProgressBar
                            android:id="@+id/pb_k"
                            style="@style/ProgressHorizontal"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/margin_padding_6dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="@dimen/margin_padding_15dp"
                            android:progress="0"
                            android:progressDrawable="@drawable/progressbar_vote"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tv_k"
                            android:layout_width="match_parent"
                            android:layout_height="33dp"
                            android:layout_alignParentBottom="true"
                            android:gravity="center"
                            android:text="后盖及左后翼子板"
                            android:textColor="@color/tv_b2b2b2"
                            android:textSize="@dimen/font_13sp" />

                    </RelativeLayout>

                    <View
                        android:layout_width="@dimen/margin_padding_10dp"
                        android:layout_height="wrap_content" />

                    <RelativeLayout
                        android:id="@+id/rl_l"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:visibility="visible">

                        <ImageView
                            android:id="@+id/iv_ll"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/margin_padding_10dp"
                            android:layout_marginTop="@dimen/margin_padding_10dp"
                            android:src="@mipmap/btn_xx"
                            android:visibility="gone" />

                        <ProgressBar
                            android:id="@+id/pb_l"
                            style="@style/ProgressHorizontal"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/margin_padding_6dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="@dimen/margin_padding_15dp"
                            android:progress="0"
                            android:progressDrawable="@drawable/progressbar_vote"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tv_l"
                            android:layout_width="match_parent"
                            android:layout_height="33dp"
                            android:layout_alignParentBottom="true"
                            android:gravity="center"
                            android:text="后盖及右后翼子板"
                            android:textColor="@color/tv_b2b2b2"
                            android:textSize="@dimen/font_13sp" />

                    </RelativeLayout>
                </LinearLayout>
            </FrameLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_padding_40dp"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/margin_padding_15dp"
                android:paddingRight="@dimen/margin_padding_15dp">

                <TextView
                    android:id="@+id/tv_up"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_padding_45dp"
                    android:layout_weight="1"
                    android:background="@drawable/edt_bg_blue_small"
                    android:gravity="center"
                    android:text="上一步"
                    android:textColor="@color/color_blue"
                    android:textSize="@dimen/font_15sp" />

                <View
                    android:layout_width="@dimen/margin_padding_10dp"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/tv_next"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_padding_45dp"
                    android:layout_weight="1"
                    android:background="@drawable/edt_bg_white_small"
                    android:gravity="center"
                    android:text="下一步"
                    android:textColor="@color/white"
                    android:textSize="@dimen/font_15sp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
